    protected override async Task OnInitializedAsync()
    {
        _editContext = new EditContext(Data);
        applicationUser = await _localStorageService.GetItem<ApplicationUser>("user");
        _dynamicForm = await Mediator.Send(new GetAllDynamicFormDropdownListQuery());
        _dynamicFormItemLine = new List<DynamicFormItemLine>();
        await UpdateDataAsync();
        StateHasChanged();
    }
    public async Task loadData(long? dynamicFormItemids)
    {
        DynamicFormItemids = dynamicFormItemids;
        await UpdateDataAsync();
    }
    public async Task UpdateDataAsync()
    {
        try
        {
            PanelVisible = true; AddEnabled = false; EditEnabled = false; DeleteEnabled = false; _dynamicFormItemLine = new List<DynamicFormItemLine>(); AttachmentEnabled = false;
            if (DynamicFormItemids > 0)
            {
                AddEnabled = true;
                _dynamicFormItemLine = await Mediator.Send(new GetAllDynamicItemFormLineList((long)DynamicFormItemids));
                if (_dynamicFormItemLine != null && _dynamicFormItemLine.Count > 0)
                {
                    PanelVisible = true;
                    _selectedItems = _dynamicFormItemLine.FirstOrDefault();

                    if (_dynamicFormItemLine != null)
                    {
                        EditEnabled = true; AttachmentEnabled = true;
                        DeleteEnabled = true;
                        StateHasChanged();
                    }
                }
            }
            PanelVisible = false;
            StateHasChanged();
        }
        catch (Exception ex)
        {

        }
    }
    void OnRowClick(GridRowClickEventArgs e)
    {
        var UniqueId = e.Grid.GetRowValue(e.VisibleIndex, "DynamicFormItemLineID");
        _selectedItems = _dynamicFormItemLine.FirstOrDefault(f => f.DynamicFormItemLineID == (long?)UniqueId);
        EditEnabled = true; AttachmentEnabled = true;
        DeleteEnabled = true;

        StateHasChanged();
    }
