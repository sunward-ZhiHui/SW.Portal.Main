    async Task OnRadioChangedItems(long? itemDynamicFormTypeID)
    {
        _dynamicformObjectDataList = new List<ExpandoObject>();
        _dynamicFormData = new List<DynamicFormData>();
        dropDownOptionsGridListModel = new DropDownOptionsGridListModel();
        SingleObject = new DynamicFormItemLineItem();
        if (itemDynamicFormTypeID > 0)
        {
            List<long?> dynamicFormIds = new List<long?>();
            dynamicFormIds.Add(itemDynamicFormTypeID);
            var list = await Mediator.Send(new GetDynamicFormItemLineDropdoenList(dynamicFormIds, applicationUser.UserID));
            if (DynamicFormItemList != null)
            {
                foreach (var v in (list.ObjectData as IEnumerable<ExpandoObject>))
                {
                    long? v1 = null; long? v2 = null; long? v3 = null;
                    foreach (var item in (dynamic)v)
                    {
                        if (item.Key == "136_AppMasterPar")
                        {
                            v1 = (long?)item.Value;
                        }
                        if (item.Key == "137_AppMasterPar")
                        {
                            v2 = (long?)item.Value;
                        }
                        if (item.Key == "138_AppMasterPar")
                        {
                            v3 = (long?)item.Value;
                        }
                    }
                    if (DynamicFormItemList.StockGroupAppParentID == v1 && DynamicFormItemList.DepartmentAppParentID == v2 && DynamicFormItemList.ItemGroupAppParentID == v3)
                    {
                        _dynamicformObjectDataList.Add(v);
                    }
                }
            }

            _dynamicFormData = list.DynamicFormListData;
            dropDownOptionsGridListModel = list;
        }
        StateHasChanged();
    }
    async Task OnRadioChangedItem(long? str, PropertyInfo property)
    {
        property.SetValue(Data, str);
        Data.ItemDynamicFormDataID = null;
        _dynamicformObjectDataList = new List<ExpandoObject>();
        _dynamicFormData = new List<DynamicFormData>();
        dropDownOptionsGridListModel = new DropDownOptionsGridListModel();
        SingleObject = new DynamicFormItemLineItem();
        if (str > 0)
