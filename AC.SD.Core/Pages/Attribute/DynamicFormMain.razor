@page "/dynamicMainForm/{DynamicFormSessionId:guid}"
@page "/dynamicMainForm/{DynamicFormSessionId:guid}/{DynamicFormDataSessionId:guid}/{DynamicFormDataGridSessionId:guid}"
@using Application.Queries;
@using global::Core.Entities;
@using MediatR;
@inject IMediator _mediator
@code {
    [Parameter]
    public Guid? DynamicFormSessionId { get; set; }
    [Parameter]
    public Guid? DynamicFormDataSessionId { get; set; }
    [Parameter]
    public Guid? DynamicFormDataGridSessionId { get; set; }
    protected override async Task OnInitializedAsync()
    {
        var result = await _mediator.Send(new GetDynamicFormApi(DynamicFormSessionId, DynamicFormDataSessionId, DynamicFormDataGridSessionId));
    }

}

