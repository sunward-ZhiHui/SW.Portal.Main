    string? errorMessage;
    string? DeleteerrorMessage;
    bool MessageVisible { get; set; }
    string MessageHeader = "Error";
    string MessageBody = "";
    private bool loading;
    bool Blukdeletepopup { get; set; } = false;
    bool EditEnabled { get; set; } = false;
    bool AddEnabled { get; set; } = false;
    private RegistrationRequestAssignmentOfJob _selectlst;
    [Parameter]
    public ApplicationUser applicationUser { get; set; }
    protected override async Task OnInitializedAsync()
    {
        _departmentItems = await Mediator.Send(new GetAllDepartmentQuery());

        await UpdateDataAsync();
    }
    void AddpopupFormClose()
    {
        InvokeAsync(UpdateDataAsync);
        EmailAddpopupForm = false;
        StateHasChanged();
    }
    void OnCheckedAllValueChanged(bool radioValue)
    {
        CheckedAll = radioValue;
        if (_dynamicFormSectionData != null && _dynamicFormSectionData.Count() > 0)
        {
            _dynamicFormSectionData.ForEach(s =>
            {
                if (s.EmailCreateSessionId != null)
                {

                }
                else
                {
                    s.IsEmailCreateDone = radioValue;
                }
            });
        }
    }
    public async Task loadChildData(RegistrationRequest dynamicFormIds)
    {
        _registrationRequestData = dynamicFormIds;
        await UpdateDataAsync();
    }
    async Task Grid_CustomizeEditModel(GridCustomizeEditModelEventArgs e)
    {
        var navitems = (RegistrationRequestAssignmentOfJob)e.EditModel;
        if (e.IsNew)
        {
        }
        else
        {
        }
    }
    async Task UpdateDataAsync()
    {
        try
        {
            PanelVisible = true; AddEnabled = false;
            EditEnabled = false; DeleteEnabled = false;
            if (_registrationRequestData != null && _registrationRequestData.RegistrationRequestId > 0)
            {
                AddEnabled = true;
                _dynamicFormSectionData = await Mediator.Send(new GetRegistrationRequestAssignmentOfJob(_registrationRequestData.RegistrationRequestId, DepartmentId));
                if (_dynamicFormSectionData.Count > 0)
                {
                    _selectedItems = _dynamicFormSectionData.FirstOrDefault();
                    EditEnabled = true;
                    DeleteEnabled = true;
                }
            }
            else
            {
                _dynamicFormSectionData = new List<RegistrationRequestAssignmentOfJob>();
            }
            PanelVisible = false;
            StateHasChanged();
        }
        catch (Exception ex)
        {

        }
    }
    void OnCustomizeCell(GridExportCustomizeCellEventArgs args)
    {
        if (args.ColumnFieldName == "ContactName" && args.AreaType == SheetAreaType.DataArea)
            args.Formatting.Font = new XlCellFont() { Italic = true };
        args.Handled = true;
    }

    async Task OnEditModelSaving(GridEditModelSavingEventArgs e)
    {

        var editModel = (RegistrationRequestAssignmentOfJob)e.EditModel;

        if (e.IsNew)
        {
            var createReq = new InsertorUpdateRegistrationRequestAssignmentOfJob
                {
                    DepartmentId = editModel.DepartmentId,
                    DetailInforamtionByGuideline = editModel.DetailInforamtionByGuideline,
                    JobNo = editModel.JobNo,
                    DetailRequirement = editModel.DetailRequirement,
                    RegistrationRequestId = _registrationRequestData.RegistrationRequestId,
                    SessionId = Guid.NewGuid(),
                    AddedByUserId = applicationUser.UserID,
                    ModifiedUserId = applicationUser.UserID,
                    ModifiedDate = DateTime.Now,
                    StatusCodeId = 1,
                    AddedDate = DateTime.Now,
                    TargetDate = editModel.TargetDate,
                    DynamicFormDataId = editModel.DynamicFormDataId,
                };
            try
            {
                var result = await Mediator.Send(createReq);
                toastService.ShowToast("Record save Successfully", AC.SD.Core.Services.ToastLevel.Success);
            }
            catch (Exception eq)
            {

                errorMessage = eq.Message;
                PopupVisible = true;

            }
            await UpdateDataAsync();


        }
        else
        {
            var UpdateReq = new InsertorUpdateRegistrationRequestAssignmentOfJob
                {
                    DepartmentId = editModel.DepartmentId,
                    DetailInforamtionByGuideline = editModel.DetailInforamtionByGuideline,
                    JobNo = editModel.JobNo,
                    DetailRequirement = editModel.DetailRequirement,
                    RegistrationRequestId = _registrationRequestData.RegistrationRequestId,
                    RegistrationRequestAssignmentOfJobId = editModel.RegistrationRequestAssignmentOfJobId,
                    SessionId = Guid.NewGuid(),
                    AddedByUserId = editModel.AddedByUserId,
                    ModifiedUserId = applicationUser.UserID,
                    ModifiedDate = DateTime.Now,
                    StatusCodeId = 1,
                    TargetDate = editModel.TargetDate,
                    AddedDate = editModel.AddedDate,
                    DynamicFormDataId = editModel.DynamicFormDataId,
                };
            var result = await Mediator.Send(UpdateReq);
            toastService.ShowToast("Record Updated Successfully", AC.SD.Core.Services.ToastLevel.Success);
            await UpdateDataAsync();

        }

    }
    private async void onShowPopupMessage(string headerText, string bodyText)
    {

        MessageVisible = true;
        MessageBody = bodyText;
        MessageHeader = headerText;
        loading = false;
        StateHasChanged();
    }
    async void ClosePopup()
    {
        SelectedDataItems = null;
        Blukdeletepopup = false;
        PopupVisible = false;
    }
    void EulaPopupClosed(PopupClosedEventArgs args)
    {
        SelectedDataItems = null;
        Blukdeletepopup = false;
        PopupVisible = false;
    }
    async Task OnDeleteDataItem(GridDataItemDeletingEventArgs e)
    {
        try
        {
            var editModel = (RegistrationRequestAssignmentOfJob)e.DataItem;
            var request = new DeleteRegistrationRequestAssignmentOfJob(editModel);
            await Mediator.Send(request);
            await UpdateDataAsync();
            SMGrid.SetFocusedRowIndex(0);
        }
        catch (Exception ex)
        {

        }
    }
    void Grid_CustomizeElement(GridCustomizeElementEventArgs e)
    {
        if (e.ElementType == GridElementType.DataRow && e.VisibleIndex % 2 == 1)
        {
            e.CssClass = "alt-item";
        }

    }
    void Onclick(DataGridSelection<RegistrationRequestAssignmentOfJob> e)
    {
    }

    void OnRowClick(GridRowClickEventArgs e)
    {
        var UniqueId = e.Grid.GetRowValue(e.VisibleIndex, "RegistrationRequestAssignmentOfJobId");
        _selectedItems = _dynamicFormSectionData.FirstOrDefault(f => f.RegistrationRequestAssignmentOfJobId == (long?)UniqueId);
        DeleteEnabled = true;
        StateHasChanged();

    }
    void OnRowDoubleClick(GridRowClickEventArgs e)
    {
        OnRowClick(e);
        addEdit();
    }
    void addNew()
    {
        SMGrid.StartEditNewRowAsync();

    }
    void addEdit()
    {
        if (IsUnsaved == true)
        {
            toastService.ShowToast("Variation No Dropdown Changed.Save after access or Reload", AC.SD.Core.Services.ToastLevel.Warning);
        }
        else
        {
            if (SelectedDataItems != null)
            {

                if (SelectedDataItems.Count == 1)
                {

                    foreach (object item in SelectedDataItems)
                    {
                        if (item is RegistrationRequestAssignmentOfJob dataselectItems)
                        {
                            _selectlst = _dynamicFormSectionData.FirstOrDefault(f => f.RegistrationRequestAssignmentOfJobId == dataselectItems.RegistrationRequestAssignmentOfJobId);
                        }
                    }
                    SMGrid.StartEditDataItemAsync(_selectlst);
                }
                else
                {
                    SMGrid.StartEditDataItemAsync(_selectedItems);
                }
            }
        }
    }
    void addDelete()
    {
        Blukdeletepopup = true;
    }
    async Task bulkDelete()
    {
        try
        {
            if (SelectedDataItems != null)
            {
                foreach (object item in SelectedDataItems)
                {
                    if (item is RegistrationRequestAssignmentOfJob selectData)
                    {

                        var request = new DeleteRegistrationRequestAssignmentOfJob(selectData);
                        await Mediator.Send(request);
                        Blukdeletepopup = false;
                        toastService.ShowToast("Record deleted Successfully", AC.SD.Core.Services.ToastLevel.Success);
                        SMGrid.SetFocusedRowIndex(0);
                    }

                }
            }
        }
        catch (Exception eq)
        {
            Blukdeletepopup = false;
            toastService.ShowToast("The Selected Record Do not deleted", AC.SD.Core.Services.ToastLevel.Error);
        }

        await UpdateDataAsync();


    }
    void SelectedDataItemsChanged(IReadOnlyList<object> SelectedToItems)
    {
        SelectedDataItems = SelectedToItems;
        if (SelectedDataItems.Count > 1)
        {
            EditEnabled = false;
        }
        else
        {
            EditEnabled = true;
        }

    }
