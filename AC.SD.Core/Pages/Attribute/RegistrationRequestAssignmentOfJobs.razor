    async Task createEmailYes()
    {
        string str = NavigationManager.Uri;
        string rep1 = str.Replace(NavigationManager.BaseUri, "").ToLower();
        string[] UrlList = rep1.Split("/");
        string backurl = rep1 + "/" + _selectedItems.SessionId;
        if (_selectedItems != null)
        {
            var insertData = new RegistrationRequestAssignmentOfJob
                {
                    AddedByUserId = applicationUser.UserID,
                    ModifiedUserId = applicationUser.UserID,
                    AddedDate = DateTime.Now,
                    ModifiedDate = DateTime.Now,
                    SessionId = _selectedItems.SessionId,
                    SubjectName = SubjectName,
                    Comment = _registrationRequestData.VariationNoInfo,
                    StatusCodeId = 1,
                    RegistrationRequestId = _selectedItems.RegistrationRequestId,
                    RegistrationRequestAssignmentOfJobId = _selectedItems.RegistrationRequestAssignmentOfJobId,
                    DepartmentId = _selectedItems.DepartmentId,
                    DepartmentUniqueSessionId = _selectedItems.DepartmentUniqueSessionId,
                    BackUrl = backurl
                };
            var response = await Mediator.Send(new InsertCreateEmailRegistrationRequestAssignmentOfJob(insertData));
            if (response.RegistrationRequestAssignmentOfJobId > 0)
            {
                toastService.ShowToast("Email created successfully.", AC.SD.Core.Services.ToastLevel.Success);
                createEmailpopup = false;
                var url = NavigationManager.BaseUri + "CreateEmail/" + _selectedItems.DepartmentUniqueSessionId;
                await jsRuntime.InvokeVoidAsync("window.open", url, "_blank");
                await UpdateDataAsync();

            }
        }
        StateHasChanged();
    }
