    async Task addDetailEmailCreate()
    {
        if (IsUnsaved == true)
        {
            toastService.ShowToast("Variation No Dropdown Changed.Save after access or Reload", AC.SD.Core.Services.ToastLevel.Warning);
        }
        else
        {
            var exitsData = _dynamicFormSectionData.Where(s => s.EmailCreateSessionId == null && s.IsEmailCreateDone == true).Count();
            if (exitsData > 0)
            {
                EmailAddpopupForm = true;
            }
            else
            {
                toastService.ShowToast("Select Checkbox For Variation No And After Click Create Email", AC.SD.Core.Services.ToastLevel.Info);
            }
        }
